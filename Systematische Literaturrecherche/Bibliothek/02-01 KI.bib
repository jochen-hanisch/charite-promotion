	keywords = {Charité:Promotion, Promotion:Literaturanalyse, Forschungsansätze, Lehr- und Lerneffektivität, Technologieintegration, Systemanpassung, Promotion:Argumentation, Lernsystemarchitektur, \#5:Zeitschriftenartikel:e-learning},
