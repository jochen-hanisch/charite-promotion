	keywords = {Charité:Promotion, Promotion:Literaturanalyse, Forschungsansätze, Lehr- und Lerneffektivität, Technologieintegration, Promotion:Argumentation, \#6:Zeitschriftenartikel:Bildung:Technologie, Promotion:FU4b, Promotion:Relevanz:3},
	file = {Krstoski und Schulz - Was Technologien ermöglichen könnten - Zur Bedeutu.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/ZVFHBMIZ/Krstoski und Schulz - Was Technologien ermöglichen könnten - Zur Bedeutu.pdf:application/pdf},
