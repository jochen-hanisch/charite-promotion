	urldate = {2024-02-29},
	journal = {Cogent Business \& Management},
	author = {Ratmono, Dwi and Sari, Ratna Candra and Warsono, Sony and Ubaidillah, Muhammad and Wibowo, Luqman Mulki},
	month = dec,
