	keywords = {Charité:Promotion, Promotion:Literaturanalyse, Lehr- und Lerneffektivität, Promotion:FU4a, Promotion:Kerngedanke, Technologieintegration, Promotion:Relevanz:4, \#0:Zeitschriftenartikel:digital:learning, Lernsystemarchitektur},
