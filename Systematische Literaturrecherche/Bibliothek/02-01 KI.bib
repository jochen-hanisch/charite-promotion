@article{watanabe_tech-savvy_2022,
	title = {The tech-savvy university: {The} implications of {AI}-supported learning for the traditional education {missionDie} technologieverliebte {Hochschule}: {Was} folgt aus dem {KI}-gestützten {Lernen} für den traditionellen {Bildungsauftrag}},
