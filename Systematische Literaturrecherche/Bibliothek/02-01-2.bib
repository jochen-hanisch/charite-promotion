	urldate = {2024-08-20},
	journal = {Journal of Electrical Systems},
	author = {Wu, Haijun},
	month = apr,
	year = {2024},
	keywords = {\#0:Zeitschriftenartikel:digital:learning, Bewertungsmethoden, Charité:Promotion, Lehr- und Lerneffektivität, Leraning:Analytics, Promotion:FU4b, Promotion:Literaturanalyse, Promotion:Relevanz:5, Promotion:Weiterführung, Technologieintegration},
	pages = {1656--1668},
