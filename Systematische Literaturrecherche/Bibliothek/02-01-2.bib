	keywords = {Lernsystemarchitektur, Bildungstheorien, Technologieintegration, Promotion:FU4a, Promotion:Relevanz:4, Promotion:Kerngedanke, Promotion:todo, Promotion:Literaturanalyse, Charité:Promotion},
