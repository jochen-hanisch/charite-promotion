@inproceedings{neugebauer_steigerung_2023,
	title = {Steigerung von {Lernerfolg} und {Motivation} durch gamifizierte {Mathematik}-{Aufgaben} in {Lernmanagementsystemen}},
	isbn = {978-3-88579-732-6},
	url = {https://dl.gi.de/handle/20.500.12116/42200},
	doi = {10/gsn7n4},
	abstract = {Da Mathematik-Kompetenz entscheidend für Studierende ist und die Punkte in Studieneingangstests sinken, ist eine gesteigerte Motivation zum Lernen nötig. Gamification kann helfen, jedoch sind viele Lösungen aufwändig und nicht erweiterbar. Eine skalierbare, quelloffene Lösung, die mit LMS-Aufgabensammlungen kompatibel ist, wäre ideal. Ein Gamification-System wird in Moodle und ILIAS mittels JavaScript integriert, und automatisch generierte Lerndaten werden als Lernpfade visualisiert. Eine Pilotierung (n=115) zeigt erhöhte Motivation und Lernerfolg bei Vorkurs-Mathematik-Aufgaben. Der Ansatz eignet sich auch für höhere Mathematik.},
