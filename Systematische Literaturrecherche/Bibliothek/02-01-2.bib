	author = {Reisenhofer, Christin and Gruber, Andreas},
	keywords = {COVID-19, Digitalisierung, Digitalization, Pandemie, Adolescent, Jugendlicher, Belastung, Humanistic pedagogics, Mediennutzung, Utilisation of media, Utilization of media, Qualitative Analyse, Qualitative analysis, Computer game, Computerspiel, Humanistische Pädagogik, Humanistische Psychologie, Personality development, Persönlichkeitsbildung, Adolescence, Adoleszenz, Playing behavior, Saving habit, Social interaction, Soziale Interaktion, Spielverhalten, Videospiel, \#7:Buchteil:digital:Medien, Promotion:Literaturanalyse, Charité:Promotion},
