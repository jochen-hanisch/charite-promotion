	pages = {139--151},
	file = {PDF:/Users/jochenhanisch-johannsen/Zotero/storage/Z2L7LPQ4/Poquet und Chen - 2024 - Integrating Theories of Learning and Social Networks in Learning Analytics.pdf:application/pdf},
