	keywords = {COVID-19, Curriculum, Humans, SARS-CoV-2, Education, Medical, Undergraduate, Lehr- und Lerneffektivität, Technologieintegration, Krisenreaktion im Bildungsbereich, Pandemics, Pathology, Betacoronavirus, Coronavirus Infections, Pneumonia, Viral, Promotion:FU1, Promotion:Relevanz:4, Promotion:Argumentation, \#0:Zeitschriftenartikel:digital:learning, Promotion:Literaturanalyse, Charité:Promotion},
