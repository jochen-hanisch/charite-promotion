@article{gallner-holzmann_visualisierung_2020,
	title = {Visualisierung und {Didaktisierung} digitaler {Archivbestände}: {Perspektiven} zur {Gestaltung} offener {Lernräume} für historisches {Lernen}},
	issn = {1424-3636, 1424-3636},
