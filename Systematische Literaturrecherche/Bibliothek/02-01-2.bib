	keywords = {Lehr- und Lerneffektivität, Technologieintegration, Forschungsansätze, Systemanpassung, Krisenreaktion im Bildungsbereich, Promotion:Argumentation, \#0:Zeitschriftenartikel:digital:learning, Promotion:Literaturanalyse, Charité:Promotion},
