	keywords = {\#0:Zeitschriftenartikel:digital:learning, Promotion:Literaturanalyse, Charité:Promotion},
	pages = {494},
	file = {Ding et al. - 2024 - The effect of simulation of sectional human anatom.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/N5EC22K7/Ding et al. - 2024 - The effect of simulation of sectional human anatom.pdf:application/pdf},
}

