	year = {2021},
	keywords = {Lernsystemarchitektur, Bewertungsmethoden, Technologieintegration, Promotion:FU3, Promotion:Relevanz:4, Promotion:Weiterführung, FernUni-Hagen:MABM:M6, \#7:Buchteil:digital:Medien, Promotion:Literaturanalyse, Charité:Promotion},
	pages = {19--30},
	file = {Dyrna und Günther - Methoden, Medien oder Werkzeuge Eine technologisc.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/NDQUPULP/Dyrna und Günther - Methoden, Medien oder Werkzeuge Eine technologisc.pdf:application/pdf},
