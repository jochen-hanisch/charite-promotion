	keywords = {Bildung, Schulentwicklung, Multimedia, Innovation, Lernfabrik, Technologieintegration, Forschungsansätze, Systemanpassung, Promotion:FU3, Promotion:Argumentation, \#9:Zeitschriftenartikel:digital:lernen, Promotion:Literaturanalyse, Charité:Promotion},
