	year = {2025},
	keywords = {Lehr- und Lerneffektivität, Bildungstheorien, Bewertungsmethoden, Technologieintegration, Kollaboratives Lernen, Systemanpassung, Promotion:FU2a, Promotion:Relevanz:4, Promotion:Kerngedanke, \#7:Zeitschriftenartikel:digital:Medien, Promotion:Literaturanalyse},
