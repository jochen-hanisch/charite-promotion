	keywords = {Technologieintegration, Forschungsansätze, Datenschutz und IT-Sicherheit, algorithmic fairness, digital healthcare, federated learning, Promotion:FU7, Promotion:Weiterführung, \#6:Zeitschriftenartikel:Bildung:Technologie, Promotion:Literaturanalyse, Charité:Promotion},
