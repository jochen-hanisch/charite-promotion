@article{rohle_digitale_2022,
	title = {Digitale und hybride {Lehre} in der medizinischen {Ausbildung} am {Beispiel} des {Medizinischen} {Interprofessionellen} {Trainingszentrums} {MITZ} {Impulse} und {Entwicklungspotentiale}},
