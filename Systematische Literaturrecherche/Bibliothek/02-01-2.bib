	pages = {12},
	file = {Wirzberger - 2019 - Pädagogik trifft Psychologie und Informatik Inter.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/RZYBUP4H/Wirzberger - 2019 - Pädagogik trifft Psychologie und Informatik Inter.pdf:application/pdf},
}

@article{schob_lernen_2020,
	title = {Lernen mit {Medien} – (k)ein nachhaltiger {Ansatz} für die {Kompetenzentwicklung} {Lehrender} im {Kontext} der {Erwachsenenbildung}/{Weiterbildung}?!},
	issn = {1424-3636, 1424-3636},
	shorttitle = {Lernen mit {Medien} – (k)ein nachhaltiger {Ansatz} für die {Kompetenzentwicklung} {Lehrender} im {Kontext} der {Erwachsenenbildung}/{Weiterbildung}?},
	url = {10.21240/mpaed/jb17/2020.05.25.X},
	doi = {10/gksnxh},
	abstract = {The use of digital media for developing the competencies of teachers in adult education/further education is a promising approach to meet the needs and requirements of the target group, to support the promotion of their professional activities and to enrich the train-the-trainer offer for teachers. At the same time, digital learning offers require a (media) didactically convincing design, which forms the basis for the use and implementation of the offer in further education practice. This article presents a digital learning offer for teachers in adult education/further education that was conceived in the course of the designbased research approach and questions its acceptance and learning effect on the basis of a formative evaluation study. A proven lasting learning effect of the offer with teachers, independent of their motivational prerequisites, as well as the identification of success factors for a resource-saving integration of the learning environment as a plug-and-play offer in practice, speak for the quality of the concept and prepare the basis for its use and multiplication within continuing education offers for teachers.},
	language = {de},
	urldate = {2021-06-20},
	journal = {MedienPädagogik: Zeitschrift für Theorie und Praxis der Medienbildung},
	author = {Schöb, Sabine},
	month = sep,
	year = {2020},
	note = {ZSCC: 0000001},
	keywords = {Lehr- und Lerneffektivität, Bildungstheorien, Technologieintegration, Forschungsansätze, Promotion:FU3, Promotion:Weiterführung, \#9:Zeitschriftenartikel:digital:lernen, Promotion:Literaturanalyse, Charité:Promotion},
	pages = {647--674},
	file = {schob_2020_lernen_mit_medien_–_(k)ein_nachhaltiger_ansatz_fur_die_kompetenzentwicklung.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/QR3ICWCA/schob_2020_lernen_mit_medien_–_(k)ein_nachhaltiger_ansatz_fur_die_kompetenzentwicklung.pdf:application/pdf},
}

