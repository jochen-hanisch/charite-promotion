@article{scheidig_lehren_2020,
	title = {Lehren lernen mit digitalen {Medien}: {Technologiegestützte} {Praxisbezüge} in der {Lehrpersonenbildung}},
	issn = {1424-3636, 1424-3636},
	shorttitle = {Lehren lernen mit digitalen {Medien}},
	url = {10.21240/mpaed/jb17/2020.05.26.X},
	doi = {10/gksnz5},
	abstract = {Professional development and the acquisition of relevant competences are main goals in pre-service teacher education. However, there is a long and controversial discourse about the (lack of) practical orientation of such study programs. While internships (especially their extension) receive a lot of attention, digital technologies in higher education such as collaborative video-annotation tools, eye-tracking systems or virtual reality simulations enable new forms of connections to professional practice. This article provides an overview of technology-based references to professional practice, presents examples and discusses their potential.},
