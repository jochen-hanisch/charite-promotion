	keywords = {\#0:Zeitschriftenartikel:digital:learning, Charité:Promotion, Promotion:Literaturanalyse},
	pages = {76},
	file = {PDF:/Users/jochenhanisch-johannsen/Zotero/storage/3U5LLDIJ/Wang et al. - 2025 - Teacher support for basic needs is associated with socio-emotional skills a self-determination theo.pdf:application/pdf},
}

@article{mohsin_student_2025,
