	editor = {{Geschäftsstelle beim Stifterverband}},
	year = {2021},
	doi = {10.1007/978-3-658-32849-8_5},
	keywords = {\#4:Buchteil:MOOC, Promotion:Literaturanalyse, Charité:Promotion},
	pages = {57--82},
	file = {Gross et al. - 2021 - Fächerunterschiede in der digitalen Hochschulbildu.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/ETANAEI9/Gross et al. - 2021 - Fächerunterschiede in der digitalen Hochschulbildu.pdf:application/pdf},
