	keywords = {Simulation, Lehr- und Lerneffektivität, Bildungstheorien, Bewertungsmethoden, Forschungsansätze, Promotion:FU4b, Promotion:FU4a, Promotion:Kerngedanke, \#6:Zeitschriftenartikel:Bildung:Technologie, Promotion:Literaturanalyse, Promotion:Eye-Tracking, Charité:Promotion, Promotion:01-02},
