	keywords = {Bildung, Multimedia, Lehr- und Lerneffektivität, Technologieintegration, Systemanpassung, Datenschutz und IT-Sicherheit, Promotion:FU4b, Promotion:Relevanz:5, Promotion:Weiterführung, \#6:Zeitschriftenartikel:Bildung:Technologie, Promotion:Literaturanalyse, Charité:Promotion},
	pages = {1--24},
	file = {Schäfer et al. - 2023 - Virtual Reality in der Schule Bedenken und Potenz.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/GEE4LP76/Schäfer et al. - 2023 - Virtual Reality in der Schule Bedenken und Potenz.pdf:application/pdf},
