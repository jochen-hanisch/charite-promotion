	author = {Jeyakumar, Tharshini and Ambata-Villanueva, Sharon and McClure, Sarah and Henderson, Carolyn and Wiljer, David},
	month = oct,
	year = {2021},
	keywords = {COVID-19, medical education, educational technology, online education, implementation, training, pedagogy, effectiveness, teaching, best practice, care delivery, development, health care providers, patient care, perception, practice, \#0:Zeitschriftenartikel:digital:learning, Promotion:Literaturanalyse, Charité:Promotion},
