	month = oct,
	year = {2023},
	keywords = {Education, Lehr- und Lerneffektivität, Bewertungsmethoden, Technologieintegration, Online learning, Promotion:FU3, Promotion:Relevanz:3, Promotion:Argumentation, Computer aided instruction, Electronic learning, accuracy of assessments, Analysis of variance, assessments, Discussion forums, Distance learning, Instruments, learning activities, \#4:Konferenz-Paper:MOOC, Promotion:Literaturanalyse, Charité:Promotion},
	pages = {1--6},
