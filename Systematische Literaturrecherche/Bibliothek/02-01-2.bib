	year = {2024},
	doi = {10.1007/978-981-97-6136-4_3},
	note = {Series Title: Lecture Notes in Educational Technology},
	keywords = {\#0:Buchteil:digital:learning, Promotion:Literaturanalyse, Charité:Promotion},
	pages = {45--67},
	file = {PDF:/Users/jochenhanisch-johannsen/Zotero/storage/8UDIX8TB/Domilescu und Mladenovici - 2024 - The Quest for More Caring and Humanity in Digital Assessment.pdf:application/pdf},
