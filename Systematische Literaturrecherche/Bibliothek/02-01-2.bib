	keywords = {\#7:Zeitschriftenartikel:digital:Medien, Promotion:Literaturanalyse, Charité:Promotion},
	pages = {70--83},
	file = {Dietrich - 2020 - Perspektiven einer Audiovisuellen Grounded-Theory-.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/THKQEQC3/Dietrich - 2020 - Perspektiven einer Audiovisuellen Grounded-Theory-.pdf:application/pdf},
