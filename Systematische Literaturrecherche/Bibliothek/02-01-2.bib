	year = {2021},
	note = {ZSCC: NoCitationData[s0]},
	keywords = {Kompetenz, Bildung, Grounded Theory, Lehrer*innenbildung, Multimedia, FernUni-Hagen:MABM:M7C, \#7:Buchteil:digital:Medien, Promotion:Literaturanalyse, Charité:Promotion},
