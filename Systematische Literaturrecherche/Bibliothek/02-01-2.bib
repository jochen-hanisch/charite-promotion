@book{zillien_digitale_2009,
	address = {Wiesbaden},
	title = {Digitale {Ungleichheit}},
	isbn = {978-3-531-16673-5 978-3-531-91493-0},
	url = {http://link.springer.com/10.1007/978-3-531-91493-0},
	abstract = {Der Einsatz von Künstlicher Intelligenz in Schulkontexten hat ür die Gestaltung von Bildungsbiografen und die Verteilung von Bildungschancen potenziell weitreichende Folgen. Der vorliegende Beitrag wirt die Frage au, wie wir das Wechselspiel von sozialen Ungleichheiten, pädagogischer Praxis und KI-gestützten echnologien analytisch so assen können, dass wir (ungewollte) Ungleichheitseekte erklären können, ohne darau zurückzuallen, involvierten Akteur:innen letztlich ehlende Einsicht in die Problematik oder gar Böswilligkeit zu unterstellen. Inspiriert von der neopragmatischen Soziologie der Kritik rücken wir Logiken und Praktiken der Klassifkation als Anker- und Ausgangspunkt einer solchen Perspektive in den Fokus. Im Fall ›algorithmischer Sortierungen‹ in Kontexten der schulischen Bildung sind diese Klassifkationspraktiken durch zwei Formen von Pluralität geprägt: erstens variierende Vorstellungen davon, was eine ›gute und gerechte Schule‹ ausmacht; zweitens divergierende Handlungslogiken in pädagogischen und technologischen Arbeitszusammenhängen. Aus dieser doppelten Pluralität ergeben sich spannungsreiche Konfgurationen, die verstehen helen, wie sich in der Verkettung von Gestaltungs- und Nutzungsormen von Bildungstechnologien beharrliche Muster der Bildungsbenachteiligung halten und entalten können.},
	language = {de},
	urldate = {2023-12-30},
	publisher = {VS Verlag für Sozialwissenschaften},
	author = {Zillien, Nicole},
	year = {2009},
