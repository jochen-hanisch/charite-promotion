	urldate = {2024-05-14},
	journal = {BMC Medical Education},
	author = {Ding, Kewen and Chen, Mingjing and Li, Ping and Xie, Zichuan and Zhang, Haorong and Kou, Ruixing and Xu, Jionghui and Zou, Ting and Luo, Zhen and Song, Haibo},
	month = may,
