	pages = {161--184},
	file = {Nickel - 2021 - Räume erschliessen mit einem Smartphone im Sachunt.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/PH4PAI64/Nickel - 2021 - Räume erschliessen mit einem Smartphone im Sachunt.pdf:application/pdf},
