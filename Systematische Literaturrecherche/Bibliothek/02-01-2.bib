	author = {Lin, Yuchen and Lemos, Martin and Neuschaefer-Rube, Christiane},
	month = nov,
	year = {2021},
	keywords = {Bildung, Digitale Bildung, mobile phone, digital learning, communication disorders, otolaryngology, phoniatrics, speech-language pathology, Multimedia, Lehr- und Lerneffektivität, Technologieintegration, Forschungsansätze, Promotion:FU3, Promotion:Argumentation, \#0:Zeitschriftenartikel:digital:learning, Promotion:Literaturanalyse, Charité:Promotion},
	pages = {e30873},
