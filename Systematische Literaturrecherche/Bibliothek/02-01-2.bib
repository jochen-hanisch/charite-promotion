@article{totter_zwischen_2020,
	title = {Zwischen traditionellem {Schulbuch} und hybridem {Lehrmittel}: {Bedingungen}, {Möglichkeiten} und {Herausforderungen} von {Lernen} und {Lehren} in einer digitalen {Welt}},
