	pages = {1--32},
	file = {Kerres - 2020 - Bildung in der digitalen Welt Über Wirkungsannahm.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/2W9DBBF2/Kerres - 2020 - Bildung in der digitalen Welt Über Wirkungsannahm.pdf:application/pdf},
