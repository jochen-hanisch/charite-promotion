	number = {12},
	urldate = {2021-12-12},
	journal = {Journal of Medical Internet Research},
	author = {Claflin, Suzi B and Campbell, Julie A and Doherty, Kathleen and Farrow, Maree and Bessing, Barnabas and Taylor, Bruce V},
	month = dec,
	year = {2021},
	keywords = {mobile phone, multiple sclerosis, health promotion, massive open online course, eHealth education, Lehr- und Lerneffektivität, Technologieintegration, Systemanpassung, Promotion:FU6, Promotion:Relevanz:4, Promotion:Argumentation, \#4:Zeitschriftenartikel:MOOC, Promotion:Literaturanalyse, Charité:Promotion},
