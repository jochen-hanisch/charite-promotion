	pmcid = {PMC10995790},
	keywords = {Tablets, Humans, Aged, dementia, aging, Lehr- und Lerneffektivität, Bewertungsmethoden, Technologieintegration, cognitive impairment, Alzheimer, mild cognitive impairment, Health Status, Touch, Cognitive Dysfunction, implicit learning, MCI, mobile digital assessments, neurodegeneration, neuropsychology, older individuals, Reaction Time, serial reaction time task, SRTT, tablet-based testing, Touch Perception, Promotion:FU1, Promotion:Relevanz:4, Promotion:Argumentation, \#0:Zeitschriftenartikel:digital:learning, Promotion:Literaturanalyse, Charité:Promotion},
	pages = {e48265},
	file = {Mychajliw et al. - 2024 - Performance Differences of a Touch-Based Serial Re.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/WTN4UFFQ/Mychajliw et al. - 2024 - Performance Differences of a Touch-Based Serial Re.pdf:application/pdf},
