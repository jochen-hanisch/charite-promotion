	note = {ZSCC: NoCitationData[s0]},
	keywords = {Lehr- und Lerneffektivität, Bildungstheorien, Technologieintegration, Forschungsansätze, Promotion:FU3, Promotion:Argumentation, \#9:Zeitschriftenartikel:digital:lernen, Promotion:Literaturanalyse, Charité:Promotion},
	pages = {709--732},
	file = {Moser - 2021 - Überlegungen zum Lernen mit und über Medien im Zei.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/BKB77PXA/Moser - 2021 - Überlegungen zum Lernen mit und über Medien im Zei.pdf:application/pdf},
