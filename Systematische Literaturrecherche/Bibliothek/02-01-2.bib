	pages = {1443--1447},
	file = {Jöbges und Meier - 2023 - Aktuelles aus der Kommunikation in der Intensivmed.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/2CBVF3SM/Jöbges und Meier - 2023 - Aktuelles aus der Kommunikation in der Intensivmed.pdf:application/pdf},
