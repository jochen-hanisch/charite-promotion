	keywords = {Lernsystemarchitektur, Lehr- und Lerneffektivität, Bewertungsmethoden, Technologieintegration, Systemanpassung, Promotion:FU1, Promotion:Relevanz:5, Promotion:Kerngedanke, \#8:Zeitschriftenartikel:blended:learning, Promotion:Literaturanalyse, Charité:Promotion},
	pages = {e34900},
