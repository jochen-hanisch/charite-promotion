	note = {ZSCC: 0000000},
	keywords = {Lehr- und Lerneffektivität, Kollaboratives Lernen, Promotion:FU2a, Promotion:Relevanz:4, Promotion:Kerngedanke, \#7:Zeitschriftenartikel:digital:Medien, Promotion:Literaturanalyse, Charité:Promotion},
