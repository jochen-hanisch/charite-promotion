	author = {Dindas, Henrik and Schulte, Frank P},
	keywords = {Lehr- und Lerneffektivität, Technologieintegration, Kollaboratives Lernen, Promotion:FU4a, Promotion:Relevanz:5, Promotion:Kerngedanke, \#9:Zeitschriftenartikel:digital:lernen, Promotion:Literaturanalyse, Charité:Promotion},
