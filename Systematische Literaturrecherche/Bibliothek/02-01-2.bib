@article{turner_uber_2022,
	title = {Über physische {Distanz} und emotionale {Nähe} im {Fernunterricht}: {Wie} kann es {Lehrer}*innen gelingen, mit {Schüler}*innen in {Kontakt} zu bleiben?},
