	file = {Lin et al. - 2024 - The effects of students’ self-efficacy, self-regul.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/5MIYFALJ/Lin et al. - 2024 - The effects of students’ self-efficacy, self-regul.pdf:application/pdf},
