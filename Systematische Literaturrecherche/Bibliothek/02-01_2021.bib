@incollection{geschaftsstelle_beim_stifterverband_methode_2021,
	address = {Wiesbaden},
	title = {„{Methode}: {Klebezettel}“?! {Design} {Thinking} und die digitale {Transformation} der {Hochschulen}. {Ein} {Beitrag} zur {Behebung} des doppelten {Technologiedefizits} der {Hochschuldidaktik}},
