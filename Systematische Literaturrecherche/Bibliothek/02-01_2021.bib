	keywords = {Charité:Promotion, Promotion:Literaturanalyse, Forschungsansätze, Lehr- und Lerneffektivität, Promotion:Kerngedanke, Systemanpassung, \#5:Zeitschriftenartikel:e-learning},
	pages = {32--40},
