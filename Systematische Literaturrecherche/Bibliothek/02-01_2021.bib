	keywords = {Charité:Promotion, Promotion:Literaturanalyse, Lehr- und Lerneffektivität, Promotion:Kerngedanke, Promotion:Relevanz:5, Technologieintegration, \#7:Zeitschriftenartikel:digital:Medien, Bildung, Multimedia, Krisenreaktion im Bildungsbereich, Promotion:FU3, FernUni-Hagen:MABM:M6, UTAUT-Modell},
	pages = {281--315},
