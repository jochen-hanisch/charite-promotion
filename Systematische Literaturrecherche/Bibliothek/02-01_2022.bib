	note = {Publisher: bwpat Förder-Club e.V.},
	keywords = {Charité:Promotion, Promotion:Literaturanalyse, Forschungsansätze, Lehr- und Lerneffektivität, Promotion:FU4a, Technologieintegration, Promotion:Argumentation, \#3:Zeitschriftenartikel:online:Lernumgebung},
	pages = {25},
