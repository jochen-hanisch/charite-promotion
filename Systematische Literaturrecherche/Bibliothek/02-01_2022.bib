@article{feigl_elisabeth_homo_2022,
	title = {Homo {Hapticus} ade? {Ein} {Nachdenken} über die {Bedeutung} von {Haptik}, {Körperlichkeit} und {Wahrnehmung} in {Zeiten} zunehmend digitalisierten {Lernens} und {Lehrens}},
