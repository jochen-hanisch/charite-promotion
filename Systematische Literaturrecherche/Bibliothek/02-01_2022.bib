	month = jun,
	year = {2022},
	keywords = {Charité:Promotion, Promotion:Literaturanalyse, Lehr- und Lerneffektivität, Technologieintegration, Bewertungsmethoden, Promotion:FU2a, Promotion:Argumentation, \#6:Zeitschriftenartikel:Bildung:Technologie, Promotion:Relevanz:3},
	pages = {40--51},
