	number = {1},
	urldate = {2023-11-18},
	journal = {BMC Medical Education},
	author = {Patja, Kristiina and Huis In ‘T Veld, Tessa and Arva, Dorottya and Bonello, Marjorie and Orhan Pees, Rana and Soethout, Marc and Van Der Esch, Martin},
	month = nov,
	year = {2022},
	keywords = {Charité:Promotion, Promotion:Literaturanalyse, Forschungsansätze, Kollaboratives Lernen, Lehr- und Lerneffektivität, Promotion:Kerngedanke, Technologieintegration, Systemanpassung, \#5:Zeitschriftenartikel:e-learning},
