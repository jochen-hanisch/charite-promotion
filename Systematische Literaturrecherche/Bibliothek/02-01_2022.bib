@article{rohs_matthias_digitalisierung_2022,
	title = {Digitalisierung als ethische {Herausforderung} für die {Erwachsenenbildung}. {Ein} {Anstoß} für die {Entwicklung} einer {Bereichsethik} aus {Sicht} der {Erwachsenenbildungsforschung}},
