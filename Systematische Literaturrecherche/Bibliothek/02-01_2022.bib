@incollection{abdul_karim_effectiveness_2022,
	address = {Cham},
	title = {The {Effectiveness} of {CANVAS} {Learning} {Management} {System} for {Teaching} {Undergraduate} {Mathematics} {During} {COVID}-19 {Pandemic}},
