@article{anselmann_lernfabriken_2022,
	title = {Lernfabriken als neuer {Lernraum} in der beruflichen {Bildung} – {Sachstandsanalyse} und {Potentiale}},
	issn = {1618-8543},
	shorttitle = {Lernfabriken als neuer {Lernraum} in der beruflichen {Bildung}},
	url = {https://www.bwpat.de/ausgabe/43/anselmann-etal},
	abstract = {Lernfabriken fungieren als komplexe Simulationen von realen Arbeits- und Produktionsabläufen, um insbesondere berufliche Handlungskompetenzen in einer „Industrie 4.0 typischen“ Facharbeit entwickeln zu können. Dabei stellen sie selbst cyberphysische Systeme dar, die an beruflichen Schulen zugleich als traditioneller Lernraum (Lernlabor) und virtueller/digitaler Lernraum zu gestalten sind („digitaler Zwilling“). Der für „digitales Lernen“ typischen räumlichen und zeitlichen Entgrenzung kann zunächst nicht entsprochen werden. Gleichwohl machen sie die Digitalität der veränderten Arbeitswelt zum Gegenstand beruflicher Lernprozesse. Dieser Beitrag reflektiert zunächst Lernfabriken als neuen Lernraum und den Anteil cyberphysischer Systeme darin. Anschließend werden anhand erster Ergebnisse einer empirischen Studie der erreichte Stand in der Implementierung und Konzeptentwicklung der beruflichen Lernfabriken an berufsbildenden Schulen in Süddeutschland aufgezeigt.},
	language = {de-DE},
	number = {43},
	urldate = {2023-02-20},
	journal = {Berufs- und Wirtschaftspädagogik - online},
	author = {Anselmann, Sebastian and Windelband, Lars and Faßhauer, Uwe},
	editor = {Büchter, Karin and Wilbers, Karl and Windelband, Lars and Gössling, Bernd},
	month = dec,
	year = {2022},
	keywords = {Charité:Promotion, Promotion:Literaturanalyse, Forschungsansätze, Technologieintegration, Systemanpassung, Promotion:Argumentation, Bildung, Multimedia, Promotion:FU3, Innovation, Schulentwicklung, \#9:Zeitschriftenartikel:digital:lernen, Lernfabrik},
	file = {Anselmann et al. - 2022 - Lernfabriken als neuer Lernraum in der beruflichen.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/WBCWCGGN/Anselmann et al. - 2022 - Lernfabriken als neuer Lernraum in der beruflichen.pdf:application/pdf},
