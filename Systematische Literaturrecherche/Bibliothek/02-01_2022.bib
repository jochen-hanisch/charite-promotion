@article{alberti_vanessa_digitale_2022,
	title = {Digitale {Kompetenzen} {Lehrender}. {Zur} {Möglichkeit} ihrer {Integration} in {Modelle} generisch pädagogischer {Kompetenzen} am {Beispiel} von {GRETA}},
