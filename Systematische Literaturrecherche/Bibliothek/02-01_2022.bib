@article{stockl_digitale_2022,
	title = {Digitale {Technik} einfach gemacht. {Der} {Einﬂuss} betrieblicher {Lerngelegenheiten} auf die {Nutzung} digitaler {Datenbanken}},
	doi = {10/gp6p8r},
	abstract = {This paper examines whether and to what extent corporate learning opportunities, and more speciﬁcally collaborative databases, can support the use and acceptance of digital technologies in corporate environments. Research on technology use and acceptance has so far focused on the characteristics of usefulness of new technologies as well as the level of education and self-efﬁcacy in technology users. Since these factors are less easy for employers to inﬂuence, this paper aims to complement previous research by examining the impact of corporate course-based and informal learning opportunities and employees’ general scope of action at their workplace. For this, 252 employees from three companies were interviewed online. We were able to show that course-based further training in particular, but also informal learning, have a strong positive effect on the perceived simplicity of digital databases.},
	language = {de-DE},
	author = {Stöckl, Andreas and Struck, Olaf},
	year = {2022},
	keywords = {Charité:Promotion, Promotion:Literaturanalyse, \#0:Zeitschriftenartikel:digital:learning},
