	journal = {BMC Medical Education},
	author = {Davies, E. Bethan and Beever, Emmeline and Glazebrook, Cris},
	month = dec,
	year = {2018},
	keywords = {Charité:Promotion, Promotion:Literaturanalyse, \#b:Zeitschriftenartikel:online:learning},
	pages = {45},
