	keywords = {\#8:Zeitschriftenartikel:blended:learning, Bewertungsmethoden, Bildung, Charité:Promotion, domestic violence, intimate partner violence, learning, Lehr- und Lerneffektivität, Multimedia, nursing education, Promotion:FU5, Promotion:Kerngedanke, Promotion:Literaturanalyse, Promotion:Relevanz:5, Technologieintegration},
	pages = {e15828},
