	pages = {125--137},
	file = {PDF:/Users/jochenhanisch-johannsen/Zotero/storage/5DRE4YBU/Dever et al. - 2024 - Emotion Theory and Learning Analytics A Theoretical Framework for Capturing Emotion Regulation Usin.pdf:application/pdf},
