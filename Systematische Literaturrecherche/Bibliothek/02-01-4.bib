	keywords = {Adult, Humans, machine learning, Machine Learning, Early Detection of Cancer, Delivery of Health Care, nasopharyngeal carcinoma, Promotion:Ausschluss, Promotion:Literaturanalyse, Charité:Promotion, head and neck cancer, Nasopharyngeal Carcinoma, Nasopharyngeal Neoplasms, prediagnostic, Promotion:S19},
	pages = {e7144},
