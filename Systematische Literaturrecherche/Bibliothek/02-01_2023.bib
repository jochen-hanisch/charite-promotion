	month = oct,
	year = {2023},
	doi = {10.1515/9783110790375-014},
	keywords = {\#7:Buchteil:digital:Medien, Charité:Promotion, Promotion:Literaturanalyse},
	pages = {115--124},
	file = {Nachreiner - 2023 - Als Fachreferentin an der Schnittstelle zwischen .pdf:/Users/jochenhanisch-johannsen/Zotero/storage/JIXQ269U/Nachreiner - 2023 - Als Fachreferentin an der Schnittstelle zwischen .pdf:application/pdf},
