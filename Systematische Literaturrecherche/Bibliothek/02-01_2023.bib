	keywords = {Charité:Promotion, Promotion:Literaturanalyse, Promotion:Kerngedanke, Promotion:FU3, \#9:Zeitschriftenartikel:digital:lernen, Promotion:FH},
	file = {Kohls und Dubbert - 2023 - Hybride Lernräume gestalten.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/NP77GFFK/Kohls und Dubbert - 2023 - Hybride Lernräume gestalten.pdf:application/pdf},
