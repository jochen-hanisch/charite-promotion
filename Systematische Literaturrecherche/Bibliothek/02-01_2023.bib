	pages = {169--182},
	file = {Kuhlmann et al. - 2023 - Digitale Hochschullehre für Alle gestalten Ergebn.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/F36597IP/Kuhlmann et al. - 2023 - Digitale Hochschullehre für Alle gestalten Ergebn.pdf:application/pdf},
