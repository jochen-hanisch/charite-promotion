	keywords = {Charité:Promotion, Promotion:Literaturanalyse, Forschungsansätze, Lehr- und Lerneffektivität, Promotion:FU4a, Promotion:Kerngedanke, Promotion:Relevanz:5, Technologieintegration, Systemanpassung, Bildungstheorien, Krisenreaktion im Bildungsbereich, COVID-19, medical education, \#5:Zeitschriftenartikel:e-learning, e-learning, flipped classroom, pathology, remote education},
