	year = {2023},
	keywords = {Charité:Promotion, Promotion:Literaturanalyse, Lehr- und Lerneffektivität, Promotion:Kerngedanke, Promotion:Relevanz:5, Technologieintegration, Bewertungsmethoden, Promotion:FU2a, Interviews, Leraning:Analytics, Usability, \#4:Konferenz-Paper:MOOC, Computer aided instruction, Electronic learning, MOOC, Anxiety disorders, Learning Analytics Dashboard, Self-regulated Learning, Supply chain management, Testing},
	pages = {1--6},
