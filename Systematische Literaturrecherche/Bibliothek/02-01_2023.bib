	editor = {Heinemann, Alisha and Karakaşoğlu, Yasemin and Linnemann, Tobias and Rose, Nadine and Sturm, Tanja},
	year = {2023},
	keywords = {Charité:Promotion, Promotion:Literaturanalyse, ⛔ No DOI found, \#0:Konferenz-Paper:digital:learning},
	file = {Breiwe et al. - „Die, die zu Hause sind Übt einfach weiter.“.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/ZN3WDQNM/Breiwe et al. - „Die, die zu Hause sind Übt einfach weiter.“.pdf:application/pdf},
}

@article{oittinen_highlighting_2023,
