	publisher = {Springer International Publishing},
	author = {Banawan, Michelle and Butterfuss, Reese and Taylor, Karen S. and Christhilf, Katerina and Hsu, Claire and O’Loughlin, Connor and Allen, Laura K. and Roscoe, Rod D. and McNamara, Danielle S.},
	editor = {Kruse, Otto and Rapp, Christian and Anson, Chris M. and Benetos, Kalliopi and Cotos, Elena and Devitt, Ann and Shibani, Antonette},
	year = {2023},
	doi = {10.1007/978-3-031-36033-6_23},
