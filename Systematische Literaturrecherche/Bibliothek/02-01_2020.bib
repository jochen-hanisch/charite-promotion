@article{mayrberger_partizipative_2020,
	title = {Partizipative {Mediendidaktik}: {Darstellung} von {Eckpunkten} und {Vertiefung} des {Partizipationsraums} als konstituierendes {Strukturelement}},
	issn = {1424-3636, 1424-3636},
	shorttitle = {Partizipative {Mediendidaktik}},
	url = {10.21240/mpaed/jb17/2020.04.26.X},
	doi = {10/gksnzj},
	abstract = {Processes of digitization can be seen as a current manifestation of the metaprocess of a mediatization due to their continuous relevance from the interactionist micro- via the institutional meso- to the social and political macro-level. In the broader sense, they describe relevant influencing factors and contextual conditions for currently all forms of teaching and learning. Media education in the narrower sense traditionally focuses on the role of educational media or media used in the educational contexts in form of applications (‹tools›) and digital technologies in the teaching and learning process. Participatory media education stands for a model of a critical-constructivist media education and ties in with general didactic considerations as well as media educational principles. It focuses on the participatory element in the analysis, planning and design of learning environments under the conditions of digitization, digitality and (deep) mediatization. In the sense of a contemporary media education, the approach refers equally to formal, non-formal and informal teaching and learning processes and is being further developed in this sense. In the present paper, the heuristic structural model of a participatory media education will be considered in an interdisciplinary way and will be discussed primarily from a scientific perspective. In an overview, conceptual cornerstones such as theoretical connections will be presented. The main focus is placed on the constituent structural element in the space for participation.},
	language = {de-DE},
	urldate = {2021-06-20},
	journal = {MedienPädagogik: Zeitschrift für Theorie und Praxis der Medienbildung},
	author = {Mayrberger, Kerstin},
	month = apr,
	year = {2020},
	note = {ZSCC: 0000005},
	keywords = {Charité:Promotion, Promotion:Literaturanalyse, Kollaboratives Lernen, Promotion:FU4a, Promotion:Kerngedanke, Promotion:Relevanz:5, Technologieintegration, Bildungstheorien, \#6:Zeitschriftenartikel:Bildung:Technologie},
	pages = {59--92},
	file = {Mayrberger - 2020 - Partizipative Mediendidaktik Darstellung von Eckp.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/XEXEACI7/Mayrberger - 2020 - Partizipative Mediendidaktik Darstellung von Eckp.pdf:application/pdf},
