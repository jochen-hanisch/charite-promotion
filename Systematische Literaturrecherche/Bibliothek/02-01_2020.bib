@article{hassounah_digital_2020,
	title = {Digital {Response} {During} the {COVID}-19 {Pandemic} in {Saudi} {Arabia}},
	volume = {22},
	issn = {1438-8871},
	url = {https://www.jmir.org/2020/9/e19338},
	doi = {10/ghqgnq},
	abstract = {Background: The first case of COVID-19 in Saudi Arabia was confirmed on March 3, 2020. Saudi Arabia, like many other countries worldwide, implemented lockdown of most public and private services in response to the pandemic and established population movement restrictions nationwide. With the implementation of these strict mitigation regulations, technology and digital solutions have enabled the provision of essential services.
Objective: The aim of this paper is to highlight how Saudi Arabia has used digital technology during the COVID-19 pandemic in the domains of public health, health care services, education, telecommunication, commerce, and risk communication.
Methods: We documented the use of digital technology in Saudi Arabia during the pandemic using publicly available official announcements, press briefings and releases, news clips, published data, peer-reviewed literature, and professional discussions.
Results: Saudi Arabia’s government and private sectors combined developed and launched approximately 19 apps and platforms that serve public health functions and provide health care services. A detailed account of each is provided. Education processes continued using an established electronic learning infrastructure with a promising direction toward wider adoption in the future. Telecommunication companies exhibited smooth collaboration as well as innovative initiatives to support ongoing efforts. Risk communication activities using social media, websites, and SMS text messaging followed best practice guides.
Conclusions: The Saudi Vision 2030 framework, released in 2017, has paved the path for digital transformation. COVID-19 enabled the promotion and testing of this transition. In Saudi Arabia, the use of artificial intelligence in integrating different data sources during future outbreaks could be further explored. Also, decreasing the number of mobile apps and merging their functions could increase and facilitate their use.},
	language = {en-GB},
	number = {9},
	urldate = {2023-02-10},
	journal = {Journal of Medical Internet Research},
	author = {Hassounah, Marwah and Raheel, Hafsa and Alhefzi, Mohammed},
	month = sep,
	year = {2020},
	keywords = {Charité:Promotion, Promotion:Literaturanalyse, \#0:Zeitschriftenartikel:digital:learning},
	pages = {e19338},
	file = {Hassounah et al. - 2020 - Digital Response During the COVID-19 Pandemic in S.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/FY8MV9S3/Hassounah et al. - 2020 - Digital Response During the COVID-19 Pandemic in S.pdf:application/pdf},
}

@article{oconnor_podcasting_2020,
	title = {Podcasting in nursing and midwifery education: {An} integrative review},
	volume = {47},
	issn = {14715953},
	shorttitle = {Podcasting in nursing and midwifery education},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1471595319309412},
	doi = {10/ghxhdv},
	abstract = {Podcasting is used in higher education so various digital resources can be shared with students. This review aims to synthesise evidence on podcasting in nursing and midwifery education. PubMed, MEDLINE, CINAHL, Scopus and ERIC databases were searched using key terms. 242 articles were found and screened. Data extraction, quality assessment and data analysis, underpinned by a Social Media Learning Model, were conducted on relevant studies. Twenty-six studies were included in the review. Three themes emerged; 1) learning and other outcomes, 2) antecedents to learning, and 3) learning process. Students seemed to acquire new knowledge and skills by using podcasts and it also appeared to improve clinical confidence. The organisation of podcasting, digital literacy and e-Professionalism, the personal motivation of learners, and flexible access to the technology seemed to impact the delivery of this educational intervention. Mechanisms that appeared to affect the learning process were the speed of exchange, the type of social media user, the timeframe, quality of information, the functionality of podcasts and other learning activities. This review synthesised evidence on podcasting in nursing and midwifery education. The technology was seen as a positive learning tool but more robust research examining its efficacy in improving learning outcomes is needed.},
	language = {en-GB},
	urldate = {2022-09-22},
	journal = {Nurse Education in Practice},
	author = {O'Connor, Siobhan and Daly, Claire S. and MacArthur, Juliet and Borglin, Gunilla and Booth, Richard G.},
	month = aug,
	year = {2020},
	keywords = {Charité:Promotion, Promotion:Literaturanalyse, \#0:Zeitschriftenartikel:digital:learning},
	pages = {102827},
	file = {O'Connor et al. - 2020 - Podcasting in nursing and midwifery education An .pdf:/Users/jochenhanisch-johannsen/Zotero/storage/4SVCKJGE/O'Connor et al. - 2020 - Podcasting in nursing and midwifery education An .pdf:application/pdf},
}

@article{waschkau_fit_2020,
	title = {[{Fit} for the {Future}: {Development} of a seminar on aspects of digitization of healthcare as a contribution of {Medical} {Sociology}]},
	volume = {155},
	issn = {2212-0289},
	shorttitle = {[{Fit} for the {Future}},
	doi = {10.1016/j.zefq.2020.06.006},
	abstract = {INTRODUCTION: Topics regarding the digitization of the healthcare system are hardly being addressed during the undergraduate training of medical students, even though the promotion of skills in this area has been defined as a learning goal in the German National Competence-Based Learning Objectives for Undergraduate Medical Education (NKLM) since 2015. At the same time, telemedicine as one part of digitization is becoming increasingly important in the public perception as a tool supplementing access to care. Therefore, the aim was to give medical students in the fourth pre-clinical semester a first insight into topics of telemedicine and other aspects of digitization in the healthcare sector as part of a seminar.
METHODS: On the basis of the existing literature, we developed the content and a didactic concept for the seminar "The doctor-patient relationship in the field of technological developments" in the Medical Sociology course for undergraduate medical students in their second year at the University of Lübeck. Inspired by Kirkpatrick's four-level model, the students were asked for topics they wished to cover regarding the digitization of healthcare and telemedicine at the start and their attitudes towards and experiences with telemedicine at the end of the seminar.
RESULTS: A total of 184 students from the fourth pre-clinical semester attended the seminar. The survey response rate was 32\%. The topics most frequently requested by students were "Legal Framework", "Electronic Patient Record" and "Tele-consultation". 85\% of the students rated the topic "digitization of healthcare" as highly relevant for their future profession. Also 85\% of the students wished for easy-to-use telemedicine equipment. A third or more of the students reported considerable uncertainty about their ability to evaluate the benefits of health apps.
CONCLUSIONS: The contents of the seminar met the students' demands. In the future, topics relating to the digitization of the health system should be increasingly integrated into undergraduate training; also, the students' feeling uncertain about their ability to assess various telemedical applications should be addressed.},
	language = {de-DE},
	journal = {Zeitschrift Fur Evidenz, Fortbildung Und Qualitat Im Gesundheitswesen},
	author = {Waschkau, Alexander and Götz, Katja and Steinhäuser, Jost},
	month = sep,
	year = {2020},
	pmid = {32798192},
	pmcid = {PMC7832042},
	note = {ZSCC: 0000003 },
	keywords = {Charité:Promotion, Promotion:Literaturanalyse, \#0:Zeitschriftenartikel:digital:learning, Germany, Sozialwissenschaft, Curriculum, Humans, Ausbildung, Digitalisierung Telemedizin, Digitization, Education, Medical, Undergraduate, eHealth, Medizinische Soziologie, Physician-Patient Relations, Students, Medical, Telemedicine, Undergraduate training},
	pages = {48--53},
	file = {Waschkau et al. - 2020 - Fit for the Future - Entwicklung eines Seminars zu.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/5WMTXZ9X/Waschkau et al. - 2020 - Fit for the Future - Entwicklung eines Seminars zu.pdf:application/pdf;waschkau_et_al_2020_[fit_for_the_future.pdf:/Users/jochenhanisch-johannsen/Zotero/storage/SISANM8X/waschkau_et_al_2020_[fit_for_the_future.pdf:application/pdf},
